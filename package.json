{
  "name": "next-admin-ts",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch --exclude ./next --exclude ./public --include './src/**/*' server.ts",
    "build": "next build",
    "start": "cross-env NODE_ENV=production tsx server.ts",
    "lint": "next lint",
    "orm:generate": "prisma generate",
    "orm:push": "prisma db push",
    "test": "vitest --run",
    "postbuild": "next-sitemap --config sitemap.config.js",
    "test:dev": "vitest",
    "test:dev-ui": "vitest --ui",
    "test:coverage": "vitest --run --coverage",
    "generate:admin": "tsx ./generate/admin.ts",
    "generate:env-types": "gen-env-types ./.env"
  },
  "prisma": {
    "schema": "orm/schema.prisma"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/prisma": "^5.0.3",
    "@faker-js/faker": "^9.5.0",
    "@sinclair/typebox": "^0.34.25",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/react": "^16.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "adminjs": "^7.8.15",
    "argon2": "^0.41.1",
    "express": "^4.21.2",
    "express-jsdoc-swagger": "^1.8.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "next": "15.1.7",
    "next-sitemap": "^4.2.3",
    "prisma": "^6.3.1",
    "prompts": "^2.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^20",
    "@types/prompts": "^2.4.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "gen-env-types": "^1.3.4",
    "jsdom": "^26.0.0",
    "nodemon": "^3.1.9",
    "ora": "^8.2.0",
    "postcss": "^8",
    "sass": "^1.85.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "vitest": "^3.0.5"
  }
}
